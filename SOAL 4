jumlah_pesan_per_domain = {}

for email in alamat_email:

    domain = email.split('@')[1]
 
    jumlah_pesan_per_domain[domain] = jumlah_pesan_per_domain.get(domain, 0) + 1

print(jumlah_pesan_per_domain)
